{
  "crons": [
    {
      "path": "/api/cron/sync-wien-info",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/scrape-wien-info?limit=200",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/cron/cache-warmup",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/generate-blog-articles",
      "schedule": "15 6 * * *"
    }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=3600"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
